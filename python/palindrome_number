class Solution:
    def isPalindrome(self, x: int) -> bool:
        return str(x) == str(x)[::-1]
        
        """
        First iteration
        
        strx = str(x)
        if x < 0:
            return False
        elif 0 < x < 10:
            return True
        else:
            for i in range(len(strx)//2):
                if int(strx[i]) != int(strx[(len(strx)-1)-i]):
                    return False
            return True
        """
        
        
